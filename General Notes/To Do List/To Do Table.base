views:
  - type: table
    name: Table
    filters:
      and:
        - file.inFolder("General Notes/To Do List/To Do Items")
    order:
      - Name
      - file.name
      - Link
      - Status
      - Tag
      - Notes
    sort:
      - property: Status
        direction: ASC
    columnSize:
      note.Name: 333
      file.name: 186
      note.Link: 164
      note.Status: 87
      note.Tag: 103
